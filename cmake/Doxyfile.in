# Doxyfile template for AWS C++ Examples
# This file will be processed by CMake to generate the actual Doxyfile

PROJECT_NAME           = "AWS SDK for C++ Examples"
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "Examples for using AWS SDK for C++"
OUTPUT_DIRECTORY       = @DOXYGEN_OUTPUT_DIR@
INPUT                  = @DOXYGEN_INPUT_DIRS@
RECURSIVE              = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
JAVADOC_AUTOBRIEF      = YES
OPTIMIZE_OUTPUT_FOR_C  = NO
BUILTIN_STL_SUPPORT    = YES
GENERATE_LATEX         = NO
GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTML_FILE_EXTENSION    = .html
GENERATE_TREEVIEW      = YES
TREEVIEW_WIDTH         = 250
GENERATE_TAGFILE       = @DOXYGEN_OUTPUT_DIR@/tagfile.tag
HAVE_DOT               = @DOXYGEN_HAVE_DOT@
DOT_IMAGE_FORMAT       = svg
INTERACTIVE_SVG        = YES
UML_LOOK               = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES
CLASS_GRAPH            = YES
COLLABORATION_GRAPH    = YES
GROUP_GRAPHS           = YES
INCLUDE_GRAPH          = YES
INCLUDED_BY_GRAPH      = YES
DIRECTORY_GRAPH        = YES
TEMPLATE_RELATIONS     = YES
SHOW_INCLUDE_FILES     = YES
USE_MDFILE_AS_MAINPAGE = @CMAKE_SOURCE_DIR@/PROJECT_README.md
